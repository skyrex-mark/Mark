    "nyc": "^13.0.1",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.7.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn test:ci"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
