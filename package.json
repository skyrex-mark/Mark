    "test:coverage": "set CI=true && react-scripts test --env=jsdom --coverage  --coverageDirectory=coverage",
    "eject": "react-scripts eject",
    "docz:dev": "docz dev",
    "docz:build": "docz build"
  },
  "devDependencies": {
    "docz": "^2.0.0",
    "husky": "^3.0.9",
    "nyc": "^14.1.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn test:ci"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
