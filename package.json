{
  "name": "mark",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "logrocket": "^1.0.3",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-particles-js": "^2.7.0",
    "react-scripts": "3.2.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
